Host mac-1
    User boinc
    ProxyCommand ssh udri -W %h:%p
    # The 'ProxyJump' directive simplifies the above 'ProxyCommand', but is only available in OpenSSH 7.3 and above... Ubuntu 16.04 LTS only has 7.2 :(
    # ProxyJump joshz@udri.mindmodeling.org
Host wsu-mm
    User root
    ProxyCommand ssh udri -W %h:%p
